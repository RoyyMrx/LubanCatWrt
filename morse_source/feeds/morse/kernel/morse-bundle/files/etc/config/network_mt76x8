config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'fd0e:e56d:8efd::/48'

config device
	option name 'br0'
	option type 'bridge'
	list ports 'eth0.1'

config device
        option name 'br-prpl'
        option type 'bridge'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0 2 6t'

config interface 'lteUsbModem'
        option proto 'dhcp'
        option device 'eth1' 